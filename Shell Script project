hive 

create database database_vgsales;
use database;
create table file_vgsales
(Rank int,
Name string,
Platform string,
Year string,
Genre string,
Publisher string,
NA_Sales string,
EU_Sales string,
JP_Sales string,
Other_Sales string,
Global_Sales string)
row format delimited
fields terminated by ','
stored as textfile
